defaults__template='template-default'
defaults__html_lang='en'                  # <html lang="${defaults__html_lang}">
defaults__html_title='Bash SSG'           # <title>${defaults__html_title}</title>
defaults__archive_old_generated='false'   # move old "generated" directory to "generated-yyyymmddhhmmss"
